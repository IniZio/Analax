{
  "name": "Analax Editor",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Live GA editor extension",
  "background": { "scripts": ["dist/background.js"] },
  "icons": { "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png" ,
    "default_title": "Analax Editor",
    "default_popup": "dist/index.html"
  },
  "content_scripts":[{
    "run_at":"document_end",
    "matches":["<all_urls>"],
    "js":["dist/popup.js"]
  }]
}
